generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  fullName  String
  email     String   @unique
  password  String
  birthDate DateTime
  role      String
  isActive  Boolean  @default(true)
  createdAt DateTime @default(now())
}
